{
    "name": "Filling out Southwest50 giveaway",
    "description": "This extension fills out the form for the Southwest giveaway. You have to navigate to the ",
    "version": "1.0",
    "manifest_version": 3,
    "options_page": "options.html",
    "icons": {
        "48": "/images/ico48.png",
        "128": "/images/ico128.png"
    },
    "background": {
        "service_worker": "background.js"
      },
      "permissions": ["storage", "activeTab", "scripting"],
      "action": {
        "default_icon": {
            "16": "/images/ico16.png",
            "32": "/images/ico32.png",
            "48": "/images/ico48.png",
            "128": "/images/ico128.png"
          }
      },
      "content_scripts":[ 
          {
              "matches": ["https://southwest50.com/wannagetaway/"],
              "js": ["content_script.js"]
          }
      ],
      "web_accessible_resources": [{
        "resources": ["showentry.js"],
        "matches": ["<all_urls>"]
      }]
}